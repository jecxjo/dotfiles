set -a
# Mark and Jump
export MARKPATH=$HOME/.marks
function jump { 
    cd -P $MARKPATH/$1 2>/dev/null || echo "No such mark: $1"
}
function mark { 
    mkdir -p $MARKPATH; ln -s $(pwd) $MARKPATH/$1
}
function unmark { 
    rm -i $MARKPATH/$1 
}
function marks {
    for f in $MARKPATH/*
    do
      echo "$(basename $f) -> $(readlink $f)" | sed 's/ -/\t-/g'
    done
    echo
}

